{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "microphone=()"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "microphone=()"
        }
        ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/"
    }
  ]
}